---
import {
  fetchOneEntry,
} from "@builder.io/sdk-react";
import BuilderContent from "../components/react/BuilderContent";

const PUBLIC_API_KEY = 'dbca46c0d48940b09ef11fa8978c6dea';

const urlPath = "/builder-demo";

const content = await fetchOneEntry({
//   options: getBuilderSearchParams(''),
  apiKey: PUBLIC_API_KEY,
  model: "page",
  userAttributes: { urlPath },
});

---
<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8"/>
<title>Builder.io Astro Demo</title>
<meta name="viewport" content="width=device-width, initial-scale=1"/>
</head>
<body>
    <h1> Hello</h1>
    <BuilderContent client:idle apiKey={PUBLIC_API_KEY} model="page" content={content} />
</body>
</html>
